              "fileReplacements": [
                {
                  "replace": "projects/coi/src/environments/environment.ts",
                  "with": "projects/coi/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "label": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "localize": ["en"],             
              "fileReplacements": [
                {
                  "replace": "projects/coi/src/environments/environment.ts",
                  "with": "projects/coi/src/environments/environment.prod.ts"
                }
              ]             
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "coi:build:production"
            },
            "development": {
              "browserTarget": "coi:build:development",
              "host": "0.0.0.0",
              "proxyConfig": "projects/coi/proxy.config.json",
              "disableHostCheck": true,
              "port": 2222
            },
            "label": {
              "browserTarget": "coi:build:development,label"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "coi:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/coi/src/test.ts",
            "polyfills": "projects/coi/src/polyfills.ts",
            "tsConfig": "projects/coi/tsconfig.spec.json",
            "karmaConfig": "projects/coi/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/coi/src/favicon.ico",
              "projects/coi/src/assets"
            ],
            "styles": [
              "projects/coi/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      },
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "en": {
            "translation": "projects/coi/src/labels/labels.xlf",
            "baseHref": ""
          }
        }
      }
    },
